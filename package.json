{
  "name": "holo-rea",
  "version": "0.0.1",
  "description": "REA economic network coordination software implemented on top of a Holochain backend.",
  "main": "/dev/null",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "postinstall": "build/postinstall.sh",
    "start": "npm-run-all --parallel dev:frontend dev:dht",
    "dev:frontend": "cd src/ui && npm start",
    "dev:dht": "docker-compose up dht",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqykly/HoloREA.git"
  },
  "keywords": [
    "REA",
    "Holochain",
    "OpenValueNetwork",
    "economic",
    "cooperative",
    "coordination"
  ],
  "author": "HoloREA team & contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sqykly/HoloREA/issues"
  },
  "homepage": "https://github.com/sqykly/HoloREA#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
